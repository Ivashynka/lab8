program lab8;

{$APPTYPE CONSOLE}

{$R *.res}
uses
  System.SysUtils;
  type tmas=array[1..1000] of integer;
TMasScore=array[1..5] of integer;
var flag:boolean;
score,CurrNumber,i,n:integer;


Function MasChecker(const MasScore_1: TMasScore; const players: integer):boolean;
var
i_1:integer;
flag_1:boolean;
begin
	flag_1:= true;
	i:=1;
	while flag and i<=players do
		begin
			if MasScore_1[i_1]>=5 then
      flag_1:=false;
			inc(i_1);
		end;
	result:=flag_1;
	end;

begin
while maschecker do
begin
  i:=1;
  while score<999 do
  begin
  readln(CurrNumber);
  score:=score+CurrNumber;
  inc(i);
  if i=n then
  i:=1;
  end;

end;



end.
